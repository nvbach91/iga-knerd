    prov:wasQuotedFrom <http://www.corestandards.org/ELA-Literacy/RL/11-12/6/> ;
    rdfs:subClassOf :CCSS.ELA-Literacy.RL.11-12 ;
    rdfs:comment "Analyze a case in which grasping a point of view requires distinguishing what is directly stated in a text from what is really meant (e.g., satire, sarcasm, irony, or understatement). "^^xsd:string .

:CCSS.ELA-Literacy.RL.11-12.7 rdf:type owl:Class ;
