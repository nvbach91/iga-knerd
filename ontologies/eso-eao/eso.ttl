    rdfs:subClassOf :CCSS.ELA-Literacy.RL.9-10 ;
    rdfs:comment "Cite strong and thorough textual evidence to support analysis of what the text says explicitly as well as inferences drawn from the text."^^xsd:string .

:CCSS.ELA-Literacy.RL.9-10.2 rdf:type owl:Class ;
