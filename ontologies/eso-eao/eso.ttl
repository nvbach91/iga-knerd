:CCSS.ELA-Literacy.RL.9-10.4 rdf:type owl:Class ;
    prov:wasQuotedFrom <http://www.corestandards.org/ELA-Literacy/RL/9-10/4/> ;
    rdfs:subClassOf :CCSS.ELA-Literacy.RL.9-10 ;
