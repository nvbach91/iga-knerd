    rdfs:subClassOf :CCSS.ELA-Literacy.RL .

:CCSS.ELA-Literacy.RL.11-12.1 rdf:type owl:Class ;
    prov:wasQuotedFrom <http://www.corestandards.org/ELA-Literacy/RL/11-12/1/> ;
