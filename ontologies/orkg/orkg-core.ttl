###  http://orkg.org/core#Paper
:Paper rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasResearchContribution ;
                         owl:someValuesFrom :ResearchContribution
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasResearchField ;
                         owl:someValuesFrom :ResearchField
                       ] ;
       rdfs:label "Research Paper"@en .


###  http://orkg.org/core#ResearchContribution
:ResearchContribution rdf:type owl:Class ;
                      rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty :addresses ;
                                                               owl:allValuesFrom :ResearchProblem
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :employs ;
                                                               owl:allValuesFrom :ResearchMethod
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :utilizes ;
                                                               owl:allValuesFrom :ResearchMaterial
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :yields ;
                                                               owl:allValuesFrom :ResearchResult
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                      rdfs:label "Research Contribution"@en .


